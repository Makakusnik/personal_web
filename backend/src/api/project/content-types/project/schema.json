{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "shortDescription": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "online",
        "offline",
        "in-progress"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "longDescriptionMd": {
      "type": "richtext"
    },
    "type": {
      "type": "string"
    },
    "imagePath": {
      "type": "string"
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill"
    },
    "opensource": {
      "type": "boolean"
    },
    "license": {
      "type": "string"
    },
    "languagesSupported": {
      "type": "string"
    },
    "launched": {
      "type": "date"
    },
    "githubUrl": {
      "type": "string"
    },
    "projectPlatforms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-platform.project-platform"
    }
  }
}
